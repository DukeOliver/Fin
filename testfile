<script>
$(window).load(function()
{
// Define the style variables
var $background_color = "#ffffff";
var $font = "'Open Sans', sans-serif;";
var $tweet_padding = "12px";
var $font_weight = "normal";
var $tweet_wrap = "normal";
var $border_radius = "0";
var $hashtag_color = "#545454";
var $tweet_border = "0";
var $tweet_border_bottom = "1px solid #e8e8e8";
var $timeline_header_padding = "5px";
var $text_color = "#545454";
var $link_color = "#4182BC";
var $name_color = "#494949";
var $exago_support_color = "#ff6b01";
var $subtext_color = "#37363a";
var $sublink_color = "#595959";
var $avatar_border = "1px solid #cccccc";
var $avatar_border_radius = "0px";
var $avatar_display = "none";
var $icon_color = "#37363a";
var $icon_hover_color = "#f36a3c";
var $header_background = "#6D8FAA";
var $header_text_color = "#ffffff";
var $follow_button_link_color = "#f36a3c";
var $display_button = "none";
var $footer_display = "none";
var $footer_padding = "0";
var $footer_background = "#f36a3c";
var $footer_tweetbox_background = "#bb5633";
var $footer_tweetbox_textcolor = "#ffffff";
var $footer_tweetbox_border = "0px";
var $load_more_background = "#273B4C";
var $load_more_text_color = "#ffffff";
    
//loads script after 300ms
  setTwitterFrameStyles();
  
  function isTwitterFrameReady()
  {
  	return $("#twitter-widget-0").contents().length > 0;
  }
  
  function setTwitterFrameStyles()
  {
  	  if (isTwitterFrameReady() === false)
      {
  	    setTimeout(function() {
  			  setTwitterFrameStyles();
        }, 300);
        return;
      }
  
// Apply the styles
$("#twitter-widget-0").contents().find('head').append('<style>.html, body, h1, h2, h3, blockquote, p, ol, ul, li, img, iframe, button, .tweet-box-button{font-family:'+$font+' !important;font-weight:'+$font_weight+' !important;} .hashtag{color:' + $hashtag_color + '!important;display:none !important;} .tweet{padding:' + $tweet_padding + ' !important;} .tweet .e-entry-title{white-space: ' + $tweet_wrap + '!important;} .customisable-border{border:' + $tweet_border + '!important;border-bottom:' + $tweet_border_bottom + '!important;} .timeline{border-radius: ' + $border_radius + '!important;} .thm-dark .retweet-credit,.h-feed, .stats strong{color:' + $text_color + ' !important;}a:not(.follow-button):not(.tweet-box-button):not(.expand):not(.u-url), .load-more{color:' + $link_color + ' ;} .follow-button{color:' + $follow_button_link_color + '; display:' + $display_button + ' !important;} .timeline-header{background:' + $header_background + '; border-radius:' + $border_radius + ' ' + $border_radius + ' 0px 0px; padding:' + $timeline_header_padding + '} .timeline-header h1 a{color:' + $header_text_color + ' !important;} .footer{display:'+$footer_display+'; padding:'+$footer_padding+'} .timeline-footer{border-radius:0px 0px ' + $border_radius + ' ' + $border_radius + ' !important; background:' + $footer_background + ' !important;} .tweet-box-button{background-color:' + $footer_tweetbox_background + ' !important; color:' + $footer_tweetbox_textcolor + ' !important; border:' + $footer_tweetbox_border + ' !important;} .timeline .stream, .tweet-actions{background:' + $background_color + ' !important;} .tweet-actions{box-shadow:0 0 10px 5px' + $background_color + ' !important;} .ic-mask{background-color:' + $icon_color + ' !important;} a:hover .ic-mask, a:focus .ic-mask{background-color:' + $icon_hover_color + ' !important;} .header .avatar{border-radius: '+ $avatar_border_radius + ' !important; border:' + $avatar_border + ' !important; display:' + $avatar_display + ' !important;} .profile[aria-label="Exago Support (screen name: ExagoSupport)"] .p-name{color:'+$exago_support_color+'!important;} .p-name{color:'+$name_color+' !important;} span.p-nickname, .u-url, .expand{color:'+$sublink_color+' !important;} .load-more, .no-more-pane {background-color:' + $load_more_background + ' !important; color:' + $load_more_text_color + '!important;} .retweet-credit{color:' + $subtext_color + ' !important;}</style>');
}
});
</script>

<!--fade in the twitter-feed-container-inner div, to combat dom flicker-->
<script>
  $(window).load(function() {
    //fade in container div
    $("#twitter-feed-container-inner").delay(300).fadeIn('slow');
  
  //hide the li items
    $("#twitter-widget-0").contents().find('li').css("display","none");
    //hide the "load more" button
    $("#twitter-widget-0").contents().find('button').css("display","none");

    //target each li item, fade at 20ms/700ms each
    $("#twitter-widget-0").contents().find('li').each(function(index) {
      console.log('success! it loaded.');
      $(this).delay(30*index).fadeIn(900);
    });

  
      //target the 'load more' button, fade in slowly
    $("#twitter-widget-0").contents().find('button').each(function(index) {
      $(this).delay(1000*index).fadeIn(1000);
    });
    
  });
</script>

